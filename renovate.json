{
  "extends": [
    "config:base"
  ],
  "dependencyDashboard": true,
  "timezone": "Asia/Shanghai",
  "schedule": "after 4am and before 8am every saturday",
  "labels": ["dependencies", "bot"],
  "packageRules": [
    {
      "matchPackageNames": [
        "HPSocket.Net"
      ],
      "allowedVersions": "<5.8.5"
    },
    {
      "matchPackageNames": [
        "Google.Protobuf"
      ],
      "allowedVersions": "<=3.19"
    }
  ]
}
